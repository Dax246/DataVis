{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": "container",
    "height": "container",
    "data": {
      "url": "country_cost_of_living.csv"
    },
    "mark": "bar",
    "encoding": {
        "x": {
            "field": "country",
            "type": "nominal",
            "title": "Country"
        },
        "y": {
          "field": "McMeal",
          "type": "quantitative",
          "title": "Cost",
          "sort": { "encoding": "x", "order": "ascending" }
        }
    } 
  }